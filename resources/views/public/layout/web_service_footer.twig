<div class="hidden">{{ state | raw}}</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/retina.js/2.1.3/retina.min.js" defer="defer"></script>
<script src="https://sdk.accountkit.com/en_US/sdk.js" defer="defer"></script>
<script src="{{ Schola.helper('assetUrl', '/public/js/landing-page.js') }}" defer="defer"></script>
<script src="{{ Schola.helper( 'assetUrl' , '/public/js/landingPage/landingPage.js' ) }}" type="text/javascript" defer="defer"></script>
<script src="{{ Schola.helper( 'assetUrl' , '/public/js/loginViaAccountKit.js' ) }}" type="text/javascript" defer="defer"></script>
<script type="text/javascript">
    var next = '{{ next }}';
    if(next !== ''){
        $('.login-popup').css({"display":"block"})
    }
    var URL = {
        register: '{{ route_url('Schola::registerTrialClass') }}',
        registerForm: '{{ route_url('Schola::registerTrialClassForm') }}',
        loginFb: '{{ route_url('Schola::login-facebook') }}',
        loginViaAccountKit: '{{ route_url('Schola::loginViaAccountKit') }}',
        childrenFillInformation: '{{ route_url('Schola::schola-children') }}',
    };
    var MESSAGE = {
        registerTrialSuccess: '{{ lang.register_trial_success }}',
        registerTrialError: '{{ lang.register_trial_error }}',
        loginFail: '{{ lang.login_fail_please_try_again }}'
    };
    var accountKitInfo = {
        fbAppId: '{{ fb_app_id }}',
        accountKitVersion: '{{ account_kit_version }}',
        accountKitNonceField: '{{ account_kit_nonce_field }}'
    }
    $(function() {
        accountKitLogin.URL = URL;
        accountKitLogin.accountKitInfo = accountKitInfo;
        accountKitLogin.messages = MESSAGE;
        accountKitLogin.init();
        landingPageView.setUrl(URL);
        landingPageView.setMessage(MESSAGE);
        landingPageView.init();
    });
    $(".youtube-video-popup").click(function(){
      $("#YourIFrameID").attr("src", $("#YourIFrameID").attr("src"));
  });
</script>